  "keywords": [
    "telegram",
    "best",
    "fontbot"
  ],
  "repository": "https://github.com/riz4d/Font-Bot",
  "env": {
    "WEBHOOK": {
      "description": "Setting this to ANYTHING will enable webhooks when in env mode",
      "value": "ANYTHING"
